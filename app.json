{
  "expo": {
    "name": "Expo and TypeScript Starter",
    "description": "Expo and TypeScript Starter (Alpha native base)",
    "slug": "expo-typescript-starter",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#fff",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#fff",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true
    }
  }
}
